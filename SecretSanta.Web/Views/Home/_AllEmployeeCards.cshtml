@model IEnumerable<SecretSanta.Models.Views.Employee>
<div class="row">
@if (Model is not null)
{
    foreach (var item in Model)
    {
        
        await Html.RenderPartialAsync("_EmployeeDetails.cshtml", item, new ViewDataDictionary(this.ViewData) { {"ShowColumns", true} });
    }
}
</div>
